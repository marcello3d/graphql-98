{
  "name": "graphql-98",
  "version": "0.1.0-rc.2",
  "private": true,
  "description": "A visual GraphQL data browser. Like a SQL GUI for GraphQL!",
  "authors": [
    {
      "name": "Marcello Bastea-Forte",
      "email": "marcello@cellosoft.com",
      "url": "https://marcello.cellosoft.com/"
    }
  ],
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts --ext tsx src"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "config": {
    "forge": {
      "packagerConfig": {
        "name": "GraphQL â€˜98",
        "executableName": "graphql98",
        "icon": "images/icon",
        "appCategoryType": "public.app-category.developer-tools",
        "darwinDarkModeSupport": true
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "graphql_98"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin",
            "win32"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/renderer/index.html",
                  "js": "./src/renderer/index.tsx",
                  "name": "app"
                }
              ]
            }
          }
        ]
      ],
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "marcello3d",
              "name": "graphql-98"
            }
          }
        }
      ]
    }
  },
  "dependencies": {
    "electron-squirrel-startup": "1.0.0"
  },
  "devDependencies": {
    "98.css": "0.1.16",
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "@electron-forge/plugin-webpack": "6.0.0-beta.54",
    "@electron-forge/publisher-github": "6.0.0-beta.54",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@reach/router": "1.3.4",
    "@types/classnames": "2.2.11",
    "@types/node": "14.14.6",
    "@types/reach__router": "1.3.6",
    "@types/react": "^16.9.55",
    "@types/react-dom": "^16.9.8",
    "@types/react-table": "7.0.25",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "camelcase": "^5.3.1",
    "classnames": "2.2.6",
    "css-loader": "^4.2.1",
    "electron": "11.0.0-beta.16",
    "electron-is-dev": "1.2.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "4.3.0",
    "graphql": "15.4.0",
    "graphql-formatter": "1.0.3",
    "graphql-hooks": "5.0.0",
    "husky": "4.3.0",
    "mini-css-extract-plugin": "0.9.0",
    "node-loader": "^1.0.1",
    "prettier": "2.1.2",
    "pretty-quick": "2.0.1",
    "query-string": "6.13.6",
    "react": "17.0.1",
    "react-dev-utils": "^10.2.1",
    "react-dom": "17.0.1",
    "react-table": "7.6.1",
    "semver": "6.3.0",
    "style-loader": "^1.2.1",
    "ts-loader": "^8.0.2",
    "typescript": "^4.0.5",
    "url-loader": "2.3.0",
    "use-query-params": "1.1.8",
    "viz.js": "2.1.2",
    "ws": "7.3.1"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
